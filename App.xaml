<Application x:Class="ItaliaPizzaClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ItaliaPizzaClient"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Properties/MergedResources.xaml"/>
                <ResourceDictionary Source="Resources/Styles/ColorPalettes.xaml"/>
                <ResourceDictionary Source="Resources/Styles/DataGridStyles.xaml"/>
                <ResourceDictionary Source="Resources/Styles/ButtonsStyles.xaml"/>
                <ResourceDictionary Source="Resources/Styles/IconsStyles.xaml"/>
                <ResourceDictionary Source="Resources/Styles/LabelsStyles.xaml"/>
                <ResourceDictionary Source="Resources/Styles/TextBoxStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
